{
  "name": "fakhrani/visitor",
  "description": "log your visitors in db, page hits, and generate visit counter for Laravel 5",
  "keywords": [
    "framework",
    "laravel",
    "visitors",
    "counter",
    "log",
    "hits",
    "clicks"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Fakhrani",
      "email": "mohammad.fakhrani@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^5.0|6.*|7.*|8.*",
    "jalle19/php-whitelist-check": "^1.0.6",
    "geoip2/geoip2": "^2.4.2"
  },
  "autoload": {
    "psr-4": {
      "Fakhrani\\Visitor\\": "src"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fakhrani\\Visitor\\VisitorServiceProvider"
      ],
      "aliases": {
        "Visitor": "Fakhrani\\Visitor\\Facades\\VisitorFacade"
      }
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
  }
}
